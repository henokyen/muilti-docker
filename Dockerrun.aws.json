{
    "AWSEBDockerrunVersion":2,
    "containerDefinitions": [
     {
         "name":"client",
         "iamge":"henok2346/multi-client",
         "hostname":"client",
         "essential":false,
         "memory":128
     },
     {
         "name":"server",
         "image":"henok2346/multi-server",
         "hostname":"api",
         "essential":false,
         "memory":128
     },
     {
        "name":"worker",
        "image":"henok2346/multi-worker",
        "hostname":"worker",
        "essential":false,
        "memory":128
     },
     {
        "name":"nginx",
        "image":"henok2346/multi-nginx",
        "essential":true,
        "postMappings":[
            {
                "hostPort":80,
                "containerPort":80
            }
        ],
        "links":["client","server"],
        "memory":128
     }
    ]
}